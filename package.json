{
  "name": "jenkins-ci-cli",
  "description": "Interactive Jenkins command line interface",
  "version": "2.0.0",
  "author": "Akshay Mittal",
  "bin": {
    "jenkins": "./bin/run"
  },
  "bugs": "https://github.com/mittalakshay6/jenkins-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/inquirer": "^7.3.3",
    "@types/jenkins": "^0.23.2",
    "@types/lodash": "^4.14.172",
    "@types/xml2js": "^0.4.9",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mittalakshay6/jenkins-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jenkins",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mittalakshay6/jenkins-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.6.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.1.2",
    "jenkins": "^0.28.1",
    "lodash": "^4.17.21",
    "tslib": "^1.14.1",
    "xml2js": "^0.4.23"
  }
}
